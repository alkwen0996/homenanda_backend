<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.home.nanda.board.model.mapper.QnAMapper">

    <resultMap type="QnA" id="QnAMap">
        <result column="qna_id" property="qnaId"/>
        <result column="user_id" property="userId"/>
        <result column="subject" property="subject"/>
        <result column="content" property="content"/>
        <result column="created_date" property="createdDate"/>
        <result column="updated_date" property="updatedDate"/>
        <result column="hit" property="hit"/>
    </resultMap>

    <select id="searchAllQnA" resultMap="QnAMap">
        select qna_id, user_id, subject, content, created_date, updated_date, hit
        from qna_board
    </select>


</mapper>
